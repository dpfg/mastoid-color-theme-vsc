			"settings": {
				"fontStyle": ""
			}
		},
		{
			"name": "Function parameter",
			"scope": "variable.parameter",
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"name": "Function argument and result types",
			"scope": "storage.type.method",
			"settings": {
				"fontStyle": "",
				"foreground": "#70727E"
			}
